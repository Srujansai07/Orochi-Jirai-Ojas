# Supabase Environment Variables
# Copy this file to .env.local and fill in your Supabase credentials

# Supabase Project URL (from Project Settings > API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anon Key (public, from Project Settings > API)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase Service Role Key (server-side only, from Project Settings > API)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database URL (from Project Settings > Database > Connection string)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-ID].supabase.co:5432/postgres
